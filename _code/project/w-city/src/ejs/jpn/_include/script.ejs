<% if (page.name == 'top') { %>
  <script src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js"></script>
  <script>
    setTimeout(() => {
      var heroSlider = new Swiper('.js-heroSlider', {
        effect: "fade",
        autoplay: {
          delay: 3000,
          disableOnInteraction: false,
          pauseOnMouseEnter: true,
        },
        loop: true,
      });
    }, 2000);
  </script>
<% } %>
<script src="<%= `${page.jpAssetsPath}assets/js/main.js` %>" defer></script>