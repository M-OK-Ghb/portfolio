		if ($("#form-embedded-subscribe-form").validationEngine("validate")) {
			$("#form-embedded-subscribe-form").submit();
			$("#form-embedded-subscribe").prop("disabled", true);
		}
	});
});

const form = document.getElementById('form-embedded-subscribe-form');
form.addEventListener('keydown', function (event) {
  if (event.keyCode === 13) {
    if (event.srcElement.type != 'submit' && 
      event.srcElement.type != 'textarea') {
      event.preventDefault();
      return false;
    }
  }
});
</script>
