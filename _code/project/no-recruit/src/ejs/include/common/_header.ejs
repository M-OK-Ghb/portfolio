<header class="l-header">
  <div class="l-header__inner">
    <div class="l-header__main">
      <h1 class="l-header__logo">
        <a href="<%= data.path %>/">
          <span class="image"><img src="<%= data.commonImgPath %>/header_logo.png" alt="NOS 日本オフィス・システム株式会社"></span>
          <span class="text">新卒採用</span> 
        </a>
      </h1>
      <ul class="l-header__globalMenu" id="js-global-menu">
        <li class="l-header__item">
          <% if (data.current === 'mission') { %>
          <div class="l-header__firstCategory --noChild current">
          <% } else { %>
          <div class="l-header__firstCategory --noChild">
          <% } %>
            <a href="<%= data.path %>/mission/">
              <span class="en">MISSION</span>
              <span class="ja">事業概要・社長メッセージ</span>
            </a>
          </div>
        </li>
        <li class="l-header__item">
          <% if (data.current === 'work') { %>
          <div class="l-header__firstCategory --hasChild current">
          <% } else { %>
          <div class="l-header__firstCategory --hasChild">
          <% } %>
            <a href="<%= data.path %>/work/" class="js-first-link">
              <span class="en">WORK</span>
              <span class="ja">NOSの仕事</span>
            </a>
            <button class="l-header__secondTrigger js-header-second-trigger">
              <span></span>
              <span></span>
            </button>
          </div>
          <ul class="l-header__secondCategory js-header-second-menu">
            <li>
              <a href="<%= data.path %>/work/">WORKトップ</a>
            </li>
            <li>
              <a href="<%= data.path %>/work/sales/">営業の仕事</a>
            </li>
            <li>
              <a href="<%= data.path %>/work/se/">SEの仕事</a>
            </li>
          </ul>
        </li>
        <li class="l-header__item">
          <% if (data.current === 'culture') { %>
          <div class="l-header__firstCategory --hasChild current">
          <% } else { %>
          <div class="l-header__firstCategory --hasChild">
          <% } %>
            <a href="<%= data.path %>/culture/" class="js-first-link">
              <span class="en">CULTURE</span>
              <span class="ja">働く環境・各種制度</span>
            </a>
            <button class="l-header__secondTrigger js-header-second-trigger">
              <span></span>
              <span></span>
            </button>
          </div>
          <ul class="l-header__secondCategory js-header-second-menu">
            <li>
              <a href="<%= data.path %>/culture/">CULTUREトップ</a>
            </li>
            <li>
              <a href="<%= data.path %>/culture/environment/">NOSの働く環境</a>
            </li>
            <li>
              <a href="<%= data.path %>/culture/welfare/">各種制度・福利厚生</a>
            </li>
            <li>
              <a href="<%= data.path %>/culture/number/">数字で見るNOS</a>
            </li>
          </ul>
        </li>
        <li class="l-header__item">
          <% if (data.current === 'people') { %>
          <div class="l-header__firstCategory --hasChild current">
          <% } else { %>
          <div class="l-header__firstCategory --hasChild">
          <% } %>
            <a href="<%= data.path %>/people/" class="js-first-link">
              <span class="en">PEOPLE</span>
              <span class="ja">社員インタビュー</span>
            </a>
            <button class="l-header__secondTrigger js-header-second-trigger">
              <span></span>
              <span></span>
            </button>
          </div>
          <ul class="l-header__secondCategory js-header-second-menu">
            <li>
              <a href="<%= data.path %>/people/">PEOPLEトップ</a>
            </li>
            <li>
              <a href="<%= data.path %>/#interview">インタビュー動画</a>
            </li>
          </ul>
        </li>
        <li class="l-header__item">
          <% if (data.current === 'faq') { %>
          <div class="l-header__firstCategory --noChild current">
          <% } else { %>
          <div class="l-header__firstCategory --noChild">
          <% } %>
            <a href="<%= data.path %>/faq/">
              <span class="en">FAQ</span>
              <span class="ja">よくある質問</span>
            </a>
          </div>
        </li>
        <li class="l-header__item">
          <% if (data.current === 'recruit') { %>
          <div class="l-header__firstCategory --hasChild current">
          <% } else { %>
          <div class="l-header__firstCategory --hasChild">
          <% } %>
            <a href="<%= data.path %>/recruit/" class="js-first-link">
              <span class="en">RECRUIT</span>
              <span class="ja">採用情報</span>
            </a>
            <button class="l-header__secondTrigger js-header-second-trigger">
              <span></span>
              <span></span>
            </button>
          </div>
          <ul class="l-header__secondCategory js-header-second-menu">
            <li>
              <a href="<%= data.path %>/recruit/">RECRUITトップ</a>
            </li>
            <li>
              <a href="<%= data.path %>/recruit/schedule/">募集要項・採用スケジュール</a>
            </li>
            <li>
              <a href="<%= data.path %>/recruit/training/">教育研修</a>
            </li>
            <li>
              <a href="<%= data.path %>/recruit/message/">採用担当からのメッセージ</a>
            </li>
          </ul>
        </li>
      </ul>
    </div>
    <div class="l-header__btnArea">
      <div class="l-header__entryTrigger">
        <button id="js-header-entry-trigger">
          <span>ENTRY</span>
          <span>
            <svg xmlns="http://www.w3.org/2000/svg" width="16.824" height="8.714" viewBox="0 0 16.824 8.714">
              <path d="M0,6.284l7.024-6S12.5,4.709,14,6.284" transform="translate(15.414 7.698) rotate(180)" fill="none" stroke="#fff" stroke-linecap="round" stroke-width="2"/>
            </svg>
          </span>
        </button>
      </div>
      <div class="l-header__menuTriggerArea">
        <button class="l-header__menuTriggerBtn" type="button" id="js-menu-trigger">
          <span></span>
          <span></span>
          <span></span>
        </button>
      </div>
    </div>
    <ul class="l-header__entryLinkList" id="js-header-entry-list">
      <li>
        <a class="l-header__entryLink myNavi" href="#" target="_blank">
          <span class="text">MYNAVI 2024</span>
          <span class="icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" viewBox="0 0 10 10">
              <path d="M0,0V10H10V7.5H8.75V8.75H1.25V1.25H2.5V0ZM5,0,6.875,1.875,3.75,5,5,6.25,8.125,3.125,10,5V0Z" fill="#fff"/>
            </svg>
          </span>
        </a>
      </li>
      <li>
        <a class="l-header__entryLink rikuNabi" href="#" target="_blank">
          <span class="text">RIKUNABI 2024</span>
          <span class="icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" viewBox="0 0 10 10">
              <path d="M0,0V10H10V7.5H8.75V8.75H1.25V1.25H2.5V0ZM5,0,6.875,1.875,3.75,5,5,6.25,8.125,3.125,10,5V0Z" fill="#fff"/>
            </svg>
          </span>
        </a>
      </li>
    </ul>
  </div>
</header>